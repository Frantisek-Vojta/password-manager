<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.passwdmanager.PasswdManagerController"
      stylesheets="@styles.css">

    <padding>
        <Insets/>
    </padding>

    <Label fx:id="welcomeText" styleClass="welcome-label"/>
    <Button text="Hello!" onAction="#onHelloButtonClick" styleClass="hello-button"/>
</VBox>

